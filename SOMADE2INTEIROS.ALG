algoritmo "Somade2Inteiros"
var
   I1, I2, S: Inteiro
inicio
      Escreva ("Informe o primiro valor: ")
      Leia (I1)
      Escreva ("Informe o segundo valor: ")
      Leia (I2)
      S <- I1 + I2
      Escreval ("O valor da soma de ", I1, " e ", I2, " e igual a ", S)
      
fimalgoritmo